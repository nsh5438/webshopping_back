<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.hs.dgsw.shop_back.Domain.CategoryMapper">
    <select id="getSubCategory" parameterType="kr.hs.dgsw.shop_back.Domain.Category" resultType="HashMap">
        select * from category where main_category = #{main_category}
    </select>

</mapper>